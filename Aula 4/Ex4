#include <stdio.h>

int main(void) {
  int p1, p2;
  printf ("Insira quantas peças devem ser produzidas: ");
  scanf ("%d", &p1);
  printf ("Insira quantas peças foram produzidas: ");
  scanf ("%d", &p2);
  if (p2 > p1){
    printf ("A produção foi bem sucedida");
  }
  if (p2<=p1){
    printf ("A produção não foi bem sucedida");
  }
    
  return 0;
}
