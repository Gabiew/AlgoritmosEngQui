#include <stdio.h>

int main(void) {
  int v[5];
  printf ("Entre com v[0]: ");
  scanf ("%d", &v[0]);
  printf ("Entre com v[1]: ");
  scanf ("%d", &v[1]);
  printf ("Entre com v[2]: ");
  scanf ("%d", &v[2]);
  printf ("Entre com v[3]: ");
  scanf ("%d", &v[3]);
  printf ("Entre com v[4]: ");
  scanf ("%d", &v[4]);

}
